{
  "name": "@onekey/sdk",
  "version": "1.0.0",
  "description": "OneKey KYC API SDK - Privacy-preserving identity verification with reusable attestations",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.esm.js",
      "require": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./crypto": {
      "import": "./dist/crypto/index.esm.js",
      "require": "./dist/crypto/index.js",
      "types": "./dist/crypto/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "kyc",
    "identity",
    "verification",
    "blockchain",
    "attestations",
    "privacy",
    "web3",
    "zero-knowledge",
    "encryption",
    "sdk",
    "typescript"
  ],
  "author": "OneKey Team",
  "license": "MIT",
  "homepage": "https://onekey.so",
  "repository": {
    "type": "git",
    "url": "https://github.com/onekey-sec/onekey-api.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/onekey-sec/onekey-api/issues"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "eventemitter3": "^5.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.10.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "@rollup/plugin-terser": "^0.4.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 